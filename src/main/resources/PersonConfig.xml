<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="person" class="com.capgemini.dev.spring.xml.Person">
		<property name="id" value="101"></property>
		<property name="name" value="Baba"></property>
		<property name="age" value="23"></property>
		<property name="job" ref="singer"></property>
	</bean>
	
	<bean id="developer" class="com.capgemini.dev.spring.xml.SoftwareDeveloper"></bean>
	
	<bean id="tester" class="com.capgemini.dev.spring.xml.SoftwareTester"></bean>
	
	<bean id="singer" class="com.capgemini.dev.spring.xml.Singer"></bean>

</beans>
